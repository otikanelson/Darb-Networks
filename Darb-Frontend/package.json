{
  "name": "darb-network-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Darb Network - Crowdfunding Platform Frontend",
  "type": "module",
  "homepage": "https://darbnetwork.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/darb-network.git"
  },
  "author": {
    "name": "Darb Network Team",
    "email": "dev@darbnetwork.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "axios": "^1.6.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.16.4",
    "react-hot-toast": "^2.4.1",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.3.3",
    "date-fns": "^2.30.0",
    "react-intersection-observer": "^9.5.2",
    "react-dropzone": "^14.2.3",
    "react-image-gallery": "^1.3.0",
    "react-circular-progressbar": "^2.1.0",
    "react-loading-skeleton": "^3.3.1",
    "react-helmet-async": "^1.3.0",
    "react-share": "^4.4.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-qr-code": "^2.0.12",
    "react-confetti": "^6.1.0",
    "recharts": "^2.8.0",
    "react-chartjs-2": "^5.2.0",
    "chart.js": "^4.4.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "dompurify": "^3.0.5",
    "js-cookie": "^3.0.5",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "clsx": "^2.0.0",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "react-virtualized-auto-sizer": "^1.0.20"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.7",
    "@types/dompurify": "^3.0.5",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-windicss": "^1.9.3",
    "vite-bundle-analyzer": "^0.7.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "vitest": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "jsdom": "^22.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "workbox-window": "^7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "crowdfunding",
    "startup",
    "investment",
    "nigeria",
    "fintech",
    "react",
    "vite"
  ],
  "bugs": {
    "url": "https://github.com/yourusername/darb-network/issues"
  }
}